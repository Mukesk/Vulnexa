{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "buildCommand": "cd vulnexa-dashboard && npm install && npm run build",
    "outputDirectory": "vulnexa-dashboard/dist",
    "framework": "vite"
}